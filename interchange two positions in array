
#include <stdio.h>
main() 
{
    int n,i,a[20],loc1,loc2,c;
    printf("enter value of n\n");
    scanf("%d",&n);
    printf("enter those elements\n");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("enter the value of locations to interchange");
    scanf("%d%d",&loc1,&loc2);
    c=a[loc1];   
    a[loc1]=a[loc2];
     a[loc2]=c;
    printf("the elements after interchanging\n");
    for(i=0;i<n;i++)
    printf("%d ",a[i]);
     

}    
    
    
    
    
    
    
    
