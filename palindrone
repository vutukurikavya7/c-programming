#include<stdio.h>
main()
{
    int n,i,d,rn=0,m;
    printf("enter value of n");
    scanf("%d",&n);
    m=n;
    for(i=1;i<=n;i++)
    {
        d=n%10;
        n=n/10;
        rn=rn*10+d;
    }

    printf("%d",rn);
if(rn==m)
    printf("palindrome");
    else
    printf("not palindrome");
    
}
