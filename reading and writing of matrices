
#include<stdio.h>
main()
{
    int a[10][10],i,j,m,n;
    printf("enter no. of rows");
    scanf("%d",&m);
    printf("enter value of n");
    scanf("%d",&n);
    printf("enter the elements");
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
       {
        scanf("%d",&a[i][j]);
       }
    }
       for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
       {
        printf("%d\t ",a[i][j]);
        }
     printf("\n");
    }
    
    
}
