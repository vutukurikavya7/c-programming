#include<stdio.h>
#include<string.h>
main()
{
    char str1[20],str2[20];
    printf("enter string1:");
    gets(str1);
    printf("enter string 2:");
    gets(str2);
    strcon(str1,str2);
    puts(str1);
    puts(str2);
    
}
    void strcon(char str1[20],char str2[20])
{
    int i=0,l1,l2;
     l1=strlen(str1);
        l2=strlen(str2);
     for(i=0;i<=l2;i++)
    {    
         str1[l1+i]=str2[i];
    }
}  
    
        
    
        
    

         
    
    
    
    
    
    
    
    

