
#include <stdio.h>
main() 
{
    int n,i,a[20],sum=0;
    printf("enter value of n\n");
    scanf("%d",&n);
    printf("enter those elements\n");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("the elements in given array are\n");
    for(i=0;i<n;i++)
    printf("%d\n",a[i]);
    for(i=0;i<n;i++)
    sum=sum+a[i];
    printf("sum=%d\n",sum);
}    
